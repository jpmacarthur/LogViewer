<UserControl x:Class="LogViewer.Viewer.ExceptionViewer"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:LogViewer.Viewer"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <Button Content="X" Panel.ZIndex="3" Height="20" Width="20" Background="Transparent" VerticalAlignment="Top" HorizontalAlignment="Right" Click="Button_Click" Visibility="Hidden" Name="exit"></Button>
        <ScrollViewer Name="Warn" Visibility="Collapsed"></ScrollViewer>
        <ListBox Name="WarnBox" MouseDoubleClick="WarnBox_MouseDoubleClick"></ListBox>
    </Grid>
</UserControl>
